{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "headers": {
        "Access-Control-Allow-Origin": "https://blog-front-seven-cyan.vercel.app",
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ]
}
